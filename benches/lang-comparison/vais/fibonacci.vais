# Fibonacci - recursive and iterative
F fib_rec(n) -> i64 =
    I n <= 1 { n } E { @(n - 1) + @(n - 2) }

F fib_iter(n) -> i64 {
    a := mut 0
    b := mut 1
    L _:0..n {
        t := a + b
        a = b
        b = t
    }
    a
}

F main() -> i64 {
    println("~{fib_rec(20)}")
    println("~{fib_iter(50)}")
    0
}
