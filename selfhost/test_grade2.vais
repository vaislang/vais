# Grade test with smaller values

F grade(score: i64) -> i64 {
    I score >= 90 {
        5  # A
    } E I score >= 80 {
        4  # B
    } E I score >= 70 {
        3  # C
    } E {
        2  # D/F
    }
}

# grade(95)=5, grade(85)=4, grade(75)=3, grade(65)=2
# Sum = 5+4+3+2 = 14
F main() -> i64 = grade(95) + grade(85) + grade(75) + grade(65)
