# Complex test for vaisc-stage1

# Function with multiple statements
F factorial(n: i64) -> i64 {
    result: mut i64 = 1
    i: mut i64 = 1
    L {
        I i > n { B } E { 0 }
        result = result * i
        i = i + 1
    }
    result
}

# Simple function
F double(x: i64) -> i64 = x * 2

# Function with if expression
F abs_val(x: i64) -> i64 {
    I x < 0 {
        0 - x
    } E {
        x
    }
}

# Nested function calls
F test_nested() -> i64 {
    double(factorial(5))
}
