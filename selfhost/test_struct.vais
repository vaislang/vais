# Test file for S/X parsing

S Point {
    x: i64,
    y: i64
}

X Point {
    F new(x: i64, y: i64) -> Point = Point { x: x, y: y }

    F get_x(&self) -> i64 = self.x

    F add(&self, other: Point) -> Point = Point {
        x: self.x + other.x,
        y: self.y + other.y
    }
}

F main() -> i64 {
    p := Point.new(10, 20)
    p.get_x()
}
