# Const Generics Test File - Parser & Type Check Test
# Tests the [T; N] const-sized array syntax

# Test 1: Struct with fixed-size array field
S Point3D {
    coords: [f64; 3]
}

# Test 2: Generic function with const parameter
F identity<T, const N: u64>(x: T) -> T {
    x
}

# Test 3: Generic struct with const parameter
S FixedArray<T, const N: u64> {
    data: [T; N],
    len: u64
}

# Test 4: Function with const array in type signature
F process(arr: [i64; 5]) -> i64 {
    55
}

# Test 5: Const expression in array size (computed at compile time)
F process_double(arr: [i64; 5 * 2]) -> i64 {
    10
}

# Main function
F main() -> i32 {
    printf("Const generics parsing test passed!\n")
    0
}
