# IO module test - simplified version using builtins directly
F main() -> i64 {
    puts("=== IO Module Test (simplified) ===")
    putchar(10)

    puts("--- Math constants (hardcoded) ---")
    puts("PI = 3.141592653589793")
    puts("E  = 2.718281828459045")
    putchar(10)

    puts("--- Arithmetic ---")
    a := 42
    b := 13
    printf("%ld + %ld = %ld\n", a, b, a + b)
    printf("%ld - %ld = %ld\n", a, b, a - b)
    printf("%ld * %ld = %ld\n", a, b, a * b)
    putchar(10)

    puts("--- Characters ---")
    putchar(72)
    putchar(101)
    putchar(108)
    putchar(108)
    putchar(111)
    putchar(10)

    puts("=== All IO tests complete! ===")
    0
}
