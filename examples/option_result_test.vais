# Test Option and Result error handling - simplified
U std/vec
U std/hashmap

F main() -> i64 {
    puts("===================================")
    puts("Option/Result Error Handling Tests")
    puts("===================================")
    puts("")

    # Test Vec basic operations
    puts("=== Testing Vec ===")
    v := vec_new()
    v.push(10)
    v.push(20)
    v.push(30)
    printf("Vec size after 3 pushes: %ld\n", v.len())
    printf("Vec[0] = %ld\n", v.get(0))
    printf("Vec[1] = %ld\n", v.get(1))
    printf("Vec[2] = %ld\n", v.get(2))
    v.drop()
    puts("")

    # Test HashMap basic operations
    puts("=== Testing HashMap ===")
    map := hashmap_new()
    map.set(1, 100)
    map.set(2, 200)
    map.set(3, 300)
    printf("HashMap.get(2) = %ld\n", map.get(2))
    printf("HashMap.get(3) = %ld\n", map.get(3))
    map.drop()
    puts("")

    puts("All tests completed!")
    0
}
