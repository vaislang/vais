# Async test (currently sync implementation)
A F compute(x: i64) -> i64 {
    x * 2
}

F main() -> i64 {
    puts("Testing async (sync for now):")

    # Call async function with await
    result := compute(21).await
    puts("compute(21).await =")
    putchar(result / 10 + 48)  # '4'
    putchar(result % 10 + 48)  # '2'
    putchar(10)

    0
}
