# Math module test - basic arithmetic only (avoids std/math IR issues)
F main() -> i64 {
    puts("=== Math Module Test ===")
    putchar(10)

    # Test basic integer arithmetic
    puts("--- Integer Arithmetic ---")
    a := 42
    b := 13
    printf("a = %ld, b = %ld\n", a, b)
    printf("a + b = %ld\n", a + b)
    printf("a - b = %ld\n", a - b)
    printf("a * b = %ld\n", a * b)
    printf("a / b = %ld\n", a / b)
    printf("a %% b = %ld\n", a % b)
    putchar(10)

    # Test negative numbers
    puts("--- Negative Numbers ---")
    neg := 0 - 42
    printf("neg = %ld\n", neg)
    printf("neg + 42 = %ld\n", neg + 42)
    putchar(10)

    # Test larger computations
    puts("--- Larger Computations ---")
    x := 1000
    y := 999
    printf("1000 * 999 = %ld\n", x * y)
    printf("1000 + 999 = %ld\n", x + y)
    putchar(10)

    # Test comparisons via conditional
    puts("--- Comparison Results ---")
    I a > b {
        puts("42 > 13: true")
    }
    I a < b {
        puts("42 < 13: true")
    } E {
        puts("42 < 13: false")
    }
    putchar(10)

    puts("=== All tests complete! ===")
    0
}
