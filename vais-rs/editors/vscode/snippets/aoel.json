{
  "Function Definition": {
    "prefix": "fn",
    "body": [
      "${1:name}(${2:params}) = ${3:body}"
    ],
    "description": "Define a function"
  },
  "Function with Block": {
    "prefix": "fnb",
    "body": [
      "${1:name}(${2:params}) = {",
      "  $0",
      "}"
    ],
    "description": "Define a function with block body"
  },
  "Async Function": {
    "prefix": "async",
    "body": [
      "async ${1:name}(${2:params}) = {",
      "  $0",
      "}"
    ],
    "description": "Define an async function"
  },
  "If Expression": {
    "prefix": "if",
    "body": [
      "if ${1:condition} then ${2:true_value} else ${3:false_value}"
    ],
    "description": "If-then-else expression"
  },
  "If Block": {
    "prefix": "ifb",
    "body": [
      "if ${1:condition} {",
      "  $0",
      "} else {",
      "  ",
      "}"
    ],
    "description": "If-else block"
  },
  "Match Expression": {
    "prefix": "match",
    "body": [
      "match ${1:value} {",
      "  ${2:pattern} => ${3:result},",
      "  _ => ${4:default}",
      "}"
    ],
    "description": "Pattern matching"
  },
  "For Loop": {
    "prefix": "for",
    "body": [
      "for ${1:item} in ${2:collection} {",
      "  $0",
      "}"
    ],
    "description": "For loop"
  },
  "Map": {
    "prefix": "map",
    "body": [
      "${1:list}.@(${2:item} => ${3:transform})"
    ],
    "description": "Map over collection"
  },
  "Filter": {
    "prefix": "filter",
    "body": [
      "${1:list}.?(${2:item} => ${3:condition})"
    ],
    "description": "Filter collection"
  },
  "Reduce": {
    "prefix": "reduce",
    "body": [
      "${1:list}./(${2:acc}, ${3:item} => ${4:combine})"
    ],
    "description": "Reduce collection"
  },
  "Pipeline": {
    "prefix": "pipe",
    "body": [
      "${1:value} |> ${2:fn1} |> ${3:fn2}"
    ],
    "description": "Pipeline operation"
  },
  "Try-Catch": {
    "prefix": "try",
    "body": [
      "try {",
      "  $1",
      "} catch ${2:e} {",
      "  $0",
      "}"
    ],
    "description": "Try-catch block"
  },
  "Type Definition": {
    "prefix": "type",
    "body": [
      "type ${1:Name} = ${2:definition}"
    ],
    "description": "Type alias"
  },
  "Enum Definition": {
    "prefix": "enum",
    "body": [
      "enum ${1:Name} {",
      "  ${2:Variant1},",
      "  ${3:Variant2}",
      "}"
    ],
    "description": "Enum definition"
  },
  "Struct Literal": {
    "prefix": "struct",
    "body": [
      "{ ${1:field}: ${2:value} }"
    ],
    "description": "Struct literal"
  },
  "Module Import": {
    "prefix": "use",
    "body": [
      "use ${1:module}"
    ],
    "description": "Import module"
  },
  "Array": {
    "prefix": "arr",
    "body": [
      "[${1:items}]"
    ],
    "description": "Array literal"
  },
  "Set": {
    "prefix": "set",
    "body": [
      "#{${1:items}}"
    ],
    "description": "Set literal"
  },
  "Map Literal": {
    "prefix": "dict",
    "body": [
      "{ \"${1:key}\": ${2:value} }"
    ],
    "description": "Map/Dictionary literal"
  },
  "Channel": {
    "prefix": "chan",
    "body": [
      "channel(${1:size})"
    ],
    "description": "Create channel"
  },
  "Spawn Task": {
    "prefix": "spawn",
    "body": [
      "spawn ${1:async_fn}(${2:args})"
    ],
    "description": "Spawn async task"
  },
  "Print": {
    "prefix": "pr",
    "body": [
      "println(${1:\"message\"})"
    ],
    "description": "Print line"
  },
  "Debug Print": {
    "prefix": "dbg",
    "body": [
      "println(\"DEBUG: ${1:name} = \" + str(${1:name}))"
    ],
    "description": "Debug print"
  }
}
